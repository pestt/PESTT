            commandId="PESTT.RemoveGraphManually">
         <activeWhen>
            <and>
               <with
                     variable="activeEditor">
                  <instanceof
                        value="org.eclipse.ui.texteditor.ITextEditor">
                  </instanceof>
               </with>
               <with
                     variable="activePartId">
                  <equals
                        value="PESTT.ViewStructuralCoverage">
                  </equals>
               </with>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="ui.handler.AddTestRequirementHandler"
            commandId="PESTT.AddPathManually">
         <activeWhen>
            <and>
               <with
                     variable="activeEditor">
                  <instanceof
                        value="org.eclipse.ui.texteditor.ITextEditor">
