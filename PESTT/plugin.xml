         <activeWhen>
            <and>
               <with
                     variable="activeEditor">
                  <instanceof
                        value="org.eclipse.ui.texteditor.ITextEditor">
                  </instanceof>
               </with>
               <with
                     variable="activePartId">
                  <equals
                        value="PESTT.ViewStructuralCoverage">
                  </equals>
